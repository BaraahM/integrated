{
  "name": "@barum/web-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "editor:dev": "next dev -p 3000 --experimental-https",
    "editor:build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "playwright test",
    "ci:checks": "./scripts/ci-checks.sh",
    "ci:checks:quick": "yarn lint:check && yarn test"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "@apollo/client": "^3.8.8",
    "@mantine/core": "^7.12.1",
    "@mantine/dropzone": "^8.1.1",
    "@mantine/form": "^7.12.1",
    "@mantine/hooks": "^7.12.1",
    "@mantine/modals": "^8.1.1",
    "@mantine/notifications": "^8.1.1",
    "@mantine/spotlight": "^8.1.1",
    "@mantine/tiptap": "^7.12.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tabler/icons-react": "^3.34.0",
    "@tiptap/extension-color": "^2.22.3",
    "@tiptap/extension-highlight": "^2.22.3",
    "@tiptap/extension-image": "^2.22.3",
    "@tiptap/extension-link": "^2.22.3",
    "@tiptap/extension-placeholder": "^2.22.3",
    "@tiptap/extension-subscript": "^2.22.3",
    "@tiptap/extension-superscript": "^2.22.3",
    "@tiptap/extension-table": "^2.22.3",
    "@tiptap/extension-table-cell": "^2.22.3",
    "@tiptap/extension-table-header": "^2.22.3",
    "@tiptap/extension-table-row": "^2.22.3",
    "@tiptap/extension-task-item": "^2.22.3",
    "@tiptap/extension-task-list": "^2.22.3",
    "@tiptap/extension-text-align": "^2.22.3",
    "@tiptap/extension-text-style": "^2.22.3",
    "@tiptap/extension-underline": "^2.22.3",
    "@tiptap/pm": "^2.22.3",
    "@tiptap/react": "^2.22.3",
    "@tiptap/starter-kit": "^2.22.3",
    "@uploadthing/react": "7.3.1",
    "ai": "^4.3.16",
    "clsx": "^2.1.1",
    "graphql": "^16.8.1",
    "luxon": "^3.4.4",
    "mantine-datatable": "^7.3.0",
    "next": "^15.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^4.0.11",
    "react-icons": "^4.12.0",
    "recharts": "^2.10.3",
    "tailwind-merge": "3.3.0",
    "uploadthing": "7.7.2",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.5",
    "@types/luxon": "^3.3.7",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "postcss-preset-mantine": "1.11.1",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/*.(test|spec).(ts|tsx|js|jsx)"
    ],
    "collectCoverageFrom": [
      "app/**/*.(ts|tsx)",
      "components/**/*.(ts|tsx)",
      "lib/**/*.(ts|tsx)",
      "services/**/*.(ts|tsx)",
      "!**/*.d.ts",
      "!app/layout.tsx",
      "!app/globals.css"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura",
      "html"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
